{
	"name": "@jmmaloney4/sector7",
	"version": "0.4.1",
	"description": "Reusable Pulumi components for infrastructure management",
	"license": "MPL-2.0",
	"publishConfig": {
		"access": "public"
	},
	"type": "commonjs",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./iam": {
			"types": "./dist/iam/index.d.ts",
			"default": "./dist/iam/index.js"
		},
		"./workersite": {
			"types": "./dist/workersite/index.d.ts",
			"default": "./dist/workersite/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rimraf dist",
		"build": "tsc -p tsconfig.json",
		"prepack": "pnpm run build"
	},
	"peerDependencies": {
		"@pulumi/pulumi": "^3.0.0",
		"@pulumi/cloudflare": "^5.0.0",
		"@pulumi/gcp": "^9.0.0"
	},
	"devDependencies": {
		"@pulumi/pulumi": "^3.197.0",
		"@pulumi/cloudflare": "^5.0.0",
		"@pulumi/gcp": "^9.0.0"
	}
}
